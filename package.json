{
  "name": "ghp",
  "version": "0.0.0",
  "author": "mironal @mironal",
  "bin": {
    "ghp": "./bin/run"
  },
  "bugs": "https://github.com/mironal/ghp/issues",
  "dependencies": {
    "@oclif/command": "1.4.34",
    "@oclif/config": "1.6.33",
    "@oclif/plugin-help": "2.0.5",
    "@octokit/rest": "15.9.4",
    "@types/shelljs": "0.8.0",
    "shelljs": "0.8.2",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.15.4",
    "@oclif/tslint": "1.1.2",
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.4",
    "@types/node": "10.5.2",
    "globby": "8.0.1",
    "ts-node": "6.2.0",
    "tslint": "5.10.0",
    "typescript": "2.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mironal/ghp",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ghp",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "mironal/ghp",
  "scripts": {
    "start": "tsc --watch",
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p . --noEmit && tslint -p . -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
